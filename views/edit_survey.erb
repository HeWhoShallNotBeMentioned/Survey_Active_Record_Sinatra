<h2><%= @survey.title() %> </h2>
<form action="/surveys/<%= @survey.id() %>" method="post">
  <input type="hidden" name="_method" value="patch">

  <label for="title">Survey Title:</label>
  <input type="text" value="<%= @survey.title() %>" name="title">

  <button type="submit" class="btn btn-primary btn-sm">Edit Survey</button>


</form>
